    <!-- Locations -->
    <entity name="ikea_small" src="resources/models/ikea_small/ikea_small.xml" scalefactor="0.01" position="4.39 0.1 0" semantic="4345430016">
      <entity name="shelf_2_1" position ="0 0 0.2" semantic="4345430272">
        <entity name="red-ribbon" src="resources/models/materials/red_ribbon.xml" scalefactor="0.01" position="0 0 0" semantic="8725200901"></entity>
        <entity name="blue-ribbon" src="resources/models/materials/blue_ribbon.xml" scalefactor="0.01" position="0 0 0.1" semantic="8725200905"></entity>

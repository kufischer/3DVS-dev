    <item name="ikea_large" src="resources/models/ikea_large/ikea_large.xml" scalefactor="0.01" position="5.2 0.1 0" location=""></item>
    <item name="reception" src="resources/models/reception/reception.xml" scalefactor="1" position="2.0 1.3 0"></item>
